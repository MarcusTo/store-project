generator client {
  provider = "mysql1"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model User{
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}